
<div>
    <h1>Productos</h1>
    {{#if products.length}}
    <ul>
        {{#each products}}
        <li>
            <strong>{{this.title}}</strong> -${{this.price}} (ID : {{this.id}})
        </li>
        {{/each}}
    </ul>
    {{else}}
      <p>No hay productos cargados</p>
      {{/if}}
</div>